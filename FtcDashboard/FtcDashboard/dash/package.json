{
  "name": "ftc-dashboard",
  "version": "0.4.15",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.2",
    "@tailwindcss/forms": "^0.5.3",
    "clsx": "^1.2.1",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-grid-layout": "^1.3.4",
